---

import { Header } from '../components/Header.tsx';
import Footer from "../components/Footer.astro"
import "./styles.css"

interface Props {
	title:string;
}

const { title="" } = Astro.props

---
<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>{title}</title>
</head>
<body>
	<Header page={title} client:load/>
	<slot />
	<Footer/>
</body>
</html>